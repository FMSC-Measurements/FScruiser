        <MenuItem Text="Data Entry1" />
    </ViewCell.ContextActions>
    <StackLayout Orientation="Horizontal">
        <Label Text ="{Binding CuttingUnitCode}" HorizontalOptions="FillAndExpand" />
        <Button
            Text = "Data Entry2"
            Command="{Binding ShowDataEntryCommand}"
            CommandParameter="{Binding .}" />
    </StackLayout>
</ViewCell>